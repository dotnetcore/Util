@model ApplicationDto

<util-form id="form" on-submit="submit(form,btnSubmit)">
    <util-layout flex="70%" x-align="Center">
        <util-card>
            <util-card-header>
                编辑应用程序
            </util-card-header>
            <util-card-content class="form-content">
                <util-layout flex="100%">
                    <util-textbox for="Code"></util-textbox>
                </util-layout>
                <util-layout flex="100%">
                    <util-textbox for="Name"></util-textbox>
                </util-layout>
                <util-layout flex="100%">
                    <util-textbox for="Comment"></util-textbox>
                </util-layout>
                <util-layout class="mt30">
                    <util-slide-toggle for="Enabled"></util-slide-toggle>
                </util-layout>
                <util-layout class="mt30">
                    <util-slide-toggle for="RegisterEnabled"></util-slide-toggle>
                </util-layout>
                <util-layout flex="100%" class="mt30">
                    <ckeditor [(ngModel)]="model.comment" [config]="{skin:'moono-lisa',uiColor:'#ffffff',extraPlugins: 'uploadimage'}" [readonly]="false" name="abcd" debounce="500"></ckeditor>
                </util-layout>
            </util-card-content>
            <util-card-actions align="Right">
                <util-button text="返回" styles="Default" on-click="back()"></util-button>
                <util-button id="btnSubmit" type="Submit" color="Primary"></util-button>
            </util-card-actions>
        </util-card>
    </util-layout>
</util-form>

