{
  "ConnectionStrings": {
    "HangfireConnection": "Server=.\\sql2019;Database=HangfireTest;uid=sa;pwd=sa;MultipleActiveResultSets=true"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Hangfire": "Information",
      "Util.Scheduling.Hangfire.Sample.Jobs.TestJob1": "Trace"
    }
  },
  "Serilog": {
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "c:\\log\\sample.hangfire-.txt",
          "rollingInterval": "Day",
          "rollOnFileSizeLimit": true,
          "fileSizeLimitBytes": 102400,
          "retainedFileCountLimit": 10,
          "outputTemplate": "{LogLevel}  >> [{SourceContext}] [{Timestamp:yyyy-MM-dd HH:mm:ss.fff}] {NewLine}{Message}{NewLine}{Exception}"
        }
      }
    ]
  }
}