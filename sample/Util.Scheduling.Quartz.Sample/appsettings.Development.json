{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Quartz": "Information",
      "Util.Scheduling.Quartz.Sample.Jobs.TestJob1": "Trace"
    }
  },
  "Serilog": {
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "c:\\log\\sample.quartz-.txt",
          "rollingInterval": "Day",
          "rollOnFileSizeLimit": true,
          "fileSizeLimitBytes": 102400,
          "retainedFileCountLimit": 10,
          "outputTemplate": "{LogLevel}  >> [{SourceContext}] [{Timestamp:yyyy-MM-dd HH:mm:ss.fff}] {NewLine}{Message}{NewLine}{Exception}"
        }
      }
    ]
  }
}