<a:ObjectID>7273E68B-26C1-4C99-B2A3-15816EC970CD</a:ObjectID>
<a:Name>QualifiedQueue</a:Name>
<a:CreationDate>1452242851</a:CreationDate>
<a:Creator>I063968</a:Creator>
<a:ModificationDate>1453454568</a:ModificationDate>
<a:Modifier>I063968</a:Modifier>
<a:Comment>Computed extended attribute providing a string with qualified queue</a:Comment>
<a:TargetCategory.Type>1</a:TargetCategory.Type>
<a:ExtendedAttributeTargetItem.DataType>14</a:ExtendedAttributeTargetItem.DataType>
<a:Computed>1</a:Computed>
<a:GetMethodScript>Function %Get%(obj)

   dim queuetpl
   queuetpl= &quot;[%Queue.Owner%?%Queue.Owner.GeneratedName%.]%Queue.GeneratedName%&quot;
   
   %Get% = obj.EvaluateTextFor(queuetpl, &quot;%CurrentTargetCode%&quot;)

End Function</a:GetMethodScript>
<a:SetMethodScript>Sub %Set%(oService, value)
output &quot;value=&quot; &amp; value
&#39; value contains queue and potentially a queue owner as &quot;owner.queue&quot;
   dim arr, sQueue, sOwner

   arr = split(Replace(Replace(value, &quot;]&quot;, &quot;&quot;), &quot;[&quot;, &quot;&quot;), &quot;.&quot;)
   
   Select Case(UBound(arr)) 
      case 0:     sQueue = arr(0)
                  sOwner = &quot;&quot;
      case 1:     sQueue = arr(1) 
                  sOwner = arr(0)
      case else   sQueue = &quot;&quot;
                  sOwner = &quot;&quot;
   end select
   
   &#39;oService.SetExtendedAttributeText &quot;Queue&quot;, sQueue
   dim oQueue, oOwner
   for each oQueue in oService.Folder.GetCollectionByStereotype(&quot;Queue&quot;)
      if (LCase(oQueue.Code) = LCase(sQueue)) then
         set oOwner = oQueue.GetExtendedAttribute(&quot;Owner&quot;)
         if ((oOwner is Nothing) and (sOwner = &quot;&quot;)) then
            oService.SetExtendedAttribute &quot;Queue&quot;, oQueue
         elseif (LCase(oOwner.Code) = LCase(sOwner)) then
            oService.SetExtendedAttribute &quot;Queue&quot;, oQueue
         end if
      end if
   next

End Sub</a:SetMethodScript>
