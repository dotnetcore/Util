<a:Name>SqlListQuery</a:Name>
<a:TextTargetItem.Value>{ OWNER, ADT, DTTPCODE, LENGTH, PREC, Type, Mandatory }

select 
   u.name, 
   t.name, 
   s.name,
   t.precision,
   t.scale,
   &#39;CLR&#39;,
   ~ t.is_nullable
from
   [%CATALOG%.]sys.types t
   join [%CATALOG%.]sys.schemas u on (u.schema_id = t.schema_id)
   join [%CATALOG%.]sys.types s on (s.system_type_id = t.system_type_id and ((s.system_type_id = s.user_type_id) or (t.is_assembly_type=1 and t.name=s.name)))
where 
   t.is_assembly_type = 1
[  and u.name=%.q:SCHEMA%]
order by 1, 2</a:TextTargetItem.Value>
