On Error Resume Next 
   Dim CONNECTION
   CONNECTION = obj.getExtendedAttribute(ADODotNetTarget + &quot;.ConnectionString&quot;)
   If CONNECTION=&quot;&quot; then
      MsgBox &quot;No connection string defined.&quot;, vbExclamation, &quot;PowerDesigner - Warning&quot;
