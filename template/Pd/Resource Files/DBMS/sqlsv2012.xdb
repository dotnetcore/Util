<o:TextTargetItem Id="o127">
<a:ObjectID>57F57F67-A7D1-40CB-B5E0-55E680700694</a:ObjectID>
<a:Name>SqlCompressionQuery</a:Name>
<a:TextTargetItem.Value>{OWNER ID, TABLE ID, INDEX ID, WithCompression ...}

select 
   u.name,
   o.name,
   i.name,
    case when(p.data_compression &gt; 0) 
         then &#39;data_compression = &#39; 
            + lower(p.data_compression_desc)
            + &#39; on partitions (&#39;
            + convert(varchar, p.partition_number)
            + &#39;), &#39;
         else &#39;&#39;
      end
from 
   [%CATALOG%.]sys.objects   o 
   join [%CATALOG%.]sys.schemas u on (u.schema_id = o.schema_id)
   join [%CATALOG%.]sys.indexes i on (i.object_id = o.object_id and i.index_id &gt; 0 and i.type != 4)
   join [%CATALOG%.]sys.partitions p on (p.object_id = o.object_id and p.index_id = i.index_id)
where 
      o.type = &#39;U&#39;
[  and u.name=%.q:OWNER%]
[  and o.name=%.q:TABLE%]
[  and i.name=%.q:INDEX%]
order by 1, 2, 3, p.partition_number</a:TextTargetItem.Value>
