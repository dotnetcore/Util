if .JOIN(&quot;update(%FK%)&quot;, &quot;&quot;, &quot; or&quot;)
begin
   select @numnull = (select count(*)
                      from   inserted
                      where  .JOIN(&quot;%FK% is null&quot;, &quot;and   &quot;, &quot;&quot;, &quot;)&quot;)
   if @numnull != @numrows
      if (select count(*)
          from   [%PQUALIFIER%]%PARENT% t1, inserted t2
