<a:MethodScript>Sub %Method%(obj)
   output obj
   dim rxp, str, mch
   set rxp = new regexp
   rxp.global = true
   rxp.IgnoreCase = true
   str = replace(replace(replace(obj.DefaultValue, vbTab, &quot; &quot;), vbLf, &quot; &quot;), vbCr, &quot; &quot;)
   rxp.Pattern = &quot;~next\s+value\s+for\s*([^\.]+\.)?([^~]+)~&quot;
   if(rxp.Test(str)) then
      set mch = rxp.Execute(str)(0)
      dim qlf, nam, sqn
      qlf = rxp.Replace(mch, &quot;$1&quot;)
      nam = rxp.Replace(mch, &quot;$2&quot;)
      if (left(nam, 1) = &quot;[&quot;) and (right(nam, 1) = &quot;]&quot;) then
         nam = mid(nam, 2, Len(nam) - 2)
      end if
      set sqn = obj.Model.FindChildByCode(nam, PDPDM.cls_Sequence)
      if not(sqn is Nothing) then
         set obj.Sequence = sqn
         obj.SetAttributeText &quot;DefaultValueDisplayed&quot;, &quot;&quot;
      end if
   end if
   
End Sub</a:MethodScript>
