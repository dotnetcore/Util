   end If
   Dim cnx &#39; connection object
   set cnx = CreateObject(&quot;ADODB.Connection&quot;)
   cnx.Open CONNECTION
If Err.Number &lt;&gt; 0 Then 
   MsgBox &quot;Connection failed.&quot;, vbExclamation, &quot;PowerDesigner - Error&quot;
On Error GoTo 0 
Else 
   MsgBox &quot;Connection successful&quot;, vbInformation, &quot;PowerDesigner&quot;
On Error GoTo 0 
End If 
End Sub</a:MethodScript>
</o:MethodTargetItem>
<o:MethodTargetItem Id="o1313">
<a:ObjectID>53BD3CB4-7BCF-49DB-A397-F5E8A4599B7D</a:ObjectID>
<a:Name>Show detail setting</a:Name>
<a:CreationDate>0</a:CreationDate>
