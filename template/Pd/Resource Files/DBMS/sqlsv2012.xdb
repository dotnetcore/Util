      obj.DeletePhysicalOption &quot;with/allow_row_locks=&quot;
      obj.ExecuteCustomMethod &quot;%CurrentTargetCode%.CleanWithOption&quot;
   else
      obj.SetPhysicalOptionValue &quot;with/allow_row_locks=&quot;, &quot;off&quot;
   end if
