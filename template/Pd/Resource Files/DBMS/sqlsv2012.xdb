<a:TargetCategory.Type>1</a:TargetCategory.Type>
<a:MethodScript>Sub %Method%(obj)

   Dim Sel
   Set Sel = obj.Model.CreateSelection()
   
   If not (Sel is nothing) Then
      dim col
      for each col in obj.Table.Columns
         if(LCase(col.ConceptualDataType) = &quot;spatial&quot;) then
            Sel.Objects.Add(col)
         end if
      next
      Dim selObj
      set selObj = Sel.ShowObjectPicker()
      If Not (selObj is Nothing) Then
         obj.SetExtendedAttribute &quot;%CurrentTargetCode%.IndexedColumn&quot;, selObj
      End If
      
      Sel.Delete
   End If
   
End Sub</a:MethodScript>
