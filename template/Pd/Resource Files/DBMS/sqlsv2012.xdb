<a:ObjectID>043DBBFD-651C-4C6D-A52B-9DF96C195D68</a:ObjectID>
<a:Name>SqlAttrQuery</a:Name>
<a:TextTargetItem.Value>{AGGREGATE, OWNER, Assembly, ReturnType }
select 
   so.name as &quot;AGGREGATE&quot;, 
   schema_name(so.schema_id) as &quot;OWNER&quot;,  
   object_name(so.parent_object_id)  as &quot;Assembly&quot;,
   rt.name +
   case when (rp.max_length &lt;&gt; rt.max_length) then &#39;(&#39; + LTRIM(STR(case when rp.system_type_id in (99, 231, 239) then (rp.max_length/2) else rp.max_length end)) + &#39;)&#39; 
        when (rp.scale &lt;&gt; rt.scale) then (LTRIM(STR(rp.precision)) + &#39;,&#39; + LTRIM(STR(rp.scale)))
        when (rp.precision &lt;&gt; rt.precision) then &#39;(&#39; + STR(rp.precision) + &#39;)&#39; else &#39;&#39; end as &quot;ReturnType&quot;
from 
   [%CATALOG%.]sys.objects so,
   [%CATALOG%.]sys.parameters rp,
   [%CATALOG%.]sys.types it,
   [%CATALOG%.]sys.types rt
where 
   so.type = &#39;AF&#39; and
   rp.object_id = so.object_id and
   rp.parameter_id = 0 and
   rp.system_type_id = rt.system_type_id
[and schema_name(so.schema_id) = %.q:SCHEMA%]
[and so.name = %.q:AGGREGATE%]
order by so.name</a:TextTargetItem.Value>
