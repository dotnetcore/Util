<a:TextTargetItem.Value>{FULLTEXTCATALOG, Path, Default, FileGroup, Authorization}
select
   ftc.name as &quot;FULLTEXTCATALOG&quot;,
   ftc.path as &quot;Path&quot;,
   case (ftc.is_default) when 1 then &#39;true&#39; else &#39;false&#39; end as &quot;Default&quot;,
   case (ftc.is_accent_sensitivity_on) when 1 then &#39;on&#39; else &#39;off&#39; end as &quot;AccentSensitivity&quot;,
   fg.name as &quot;FileGroup&quot;,
   dp.name as &quot;Authorization&quot;
from
   [%CATALOG%.]sys.fulltext_catalogs ftc,
   [%CATALOG%.]sys.filegroups fg,
   [%CATALOG%.]sys.database_principals dp
where
    ftc.data_space_id = fg.data_space_id and
    ftc.principal_id = dp.principal_id
[and ftc.name = %.q:FULLTEXTCATALOG%]
order by ftc.name
</a:TextTargetItem.Value>
