<o:TextTargetItem Id="o300">
<a:ObjectID>4E1CBAA9-78B9-4166-AB2F-A38DE6BF5707</a:ObjectID>
<a:Name>ViewComment</a:Name>
<a:TextTargetItem.Value>[if exists (select 1 from  sys.extended_properties
           where major_id = object_id(&#39;[%QUALIFIER%]%VIEW%&#39;) and minor_id = 0)
begin
   [%OWNER%?[.O:[execute ][exec ]]sp_dropextendedproperty [%R%?[N]]&#39;MS_Description&#39;, 
   [%R%?[N]]&#39;user&#39;, [%R%?[N]]%.q:OWNER%, [%R%?[N]]&#39;view&#39;, [%R%?[N]]%.q:VIEW%
:declare @CurrentUser sysname
select @CurrentUser = user_name()
[.O:[execute ][exec ]]sp_dropextendedproperty [%R%?[N]]&#39;MS_Description&#39;, 
   [%R%?[N]]&#39;user&#39;, [%R%?[N]]@CurrentUser, [%R%?[N]]&#39;view&#39;, [%R%?[N]]%.q:VIEW%
]
end


][%OWNER%?[.O:[execute ][exec ]]sp_addextendedproperty [%R%?[N]]&#39;MS_Description&#39;, 
   [%R%?[N]]%.q:COMMENT%,
   [%R%?[N]]&#39;user&#39;, [%R%?[N]]%.q:OWNER%, [%R%?[N]]&#39;view&#39;, [%R%?[N]]%.q:VIEW%
:select @CurrentUser = user_name()
[.O:[execute ][exec ]]sp_addextendedproperty [%R%?[N]]&#39;MS_Description&#39;, 
   [%R%?[N]]%.q:COMMENT%,
   [%R%?[N]]&#39;user&#39;, [%R%?[N]]@CurrentUser, [%R%?[N]]&#39;view&#39;, [%R%?[N]]%.q:VIEW%
]
</a:TextTargetItem.Value>
<a:CreationDate>0</a:CreationDate>
