<a:ObjectID>032E624B-88A1-40FD-82E5-2F951F3B8A40</a:ObjectID>
<a:Name>SqlAttrQuery</a:Name>
<a:TextTargetItem.Value>{ ASSEMBLY, PermissionSet, Visibility, FileName}
select
   asm.name as ASSEMBLY,
   case (asm.permission_set) when 1 then &#39;SAFE&#39; when 2 then &#39;EXTERNAL_ACCESS&#39; when 3 then &#39;UNSAFE&#39; else &#39;&#39; end as PermissionSet,
   case (asm.is_visible) when 1 then &#39;on&#39; when 0 then &#39;off&#39; else &#39;&#39; end as Visibility,
   asf.name as FileName
from
   [%CATALOG%.]sys.assemblies asm,
   [%CATALOG%.]sys.assembly_files asf
where
   asm.assembly_id = asf.assembly_id and
   asf.file_id = 1
[and asm.name = %.q:ASSEMBLY%]
order by 1
</a:TextTargetItem.Value>
