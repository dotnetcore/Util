   with visibility = %Visibility%, unchecked data
.elsif ((%Visibility% != &quot;&quot;) or (%UncheckedData% == true) )
alter assembly %ASSEMBLY%
   with[ visibility = %Visibility%][%UncheckedData%? unchecked data]
.endif(&quot;\ngo&quot;)

</a:TextTargetItem.Value>
