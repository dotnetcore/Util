<o:TextTargetItem Id="o455">
<a:ObjectID>59DD9460-6F96-42B4-A85D-46BBD4D2C13F</a:ObjectID>
<a:Name>Options</a:Name>
<a:TextTargetItem.Value>as %s : default=integer
&lt;initial&gt; : composite=yes
{
increment by %d
start with %d
}
&lt;limits&gt; : composite=yes
{
&lt;maxvalue&gt; : composite=yes
{
maxvalue %d
&lt;nomaxvalue&gt; %s : list=nomaxvalue|no maxvalue
}
&lt;minvalue&gt; : composite=yes
{
minvalue %d
&lt;nominvalue&gt; %s : list=nominvalue|no minvalue
}
}
&lt;cycle&gt; %s : default=no cycle, list=cycle|no cycle
&lt;cache&gt; : composite=yes
{
cache : composite=yes
{
   &lt;cache_value&gt; %d : default=20
}
no cache
}
&lt;order&gt; %s : default=no order, list=order|no order</a:TextTargetItem.Value>
<a:CreationDate>0</a:CreationDate>
<a:Creator/>
<a:ModificationDate>1383831255</a:ModificationDate>
<a:Modifier>I063968</a:Modifier>
<a:Comment>Options for creating a sequence</a:Comment>
</o:TextTargetItem>
<o:TextTargetItem Id="o456">
<a:ObjectID>8BA68D87-8D78-4347-81C1-D3FF45EA6033</a:ObjectID>
<a:Name>SqlListQuery</a:Name>
<a:TextTargetItem.Value>{OWNER, SQNC, OPTIONS}

select 
   schema_name(s.schema_id),
   s.name,
   &#39;as &#39; + type_name(s.system_type_id)
   + case when (s.start_value = s.minimum_value) then &#39;&#39; else &#39; start with &#39; + convert(varchar(30), s.start_value) end
   + case when (s.increment = 1) then &#39;&#39; else &#39; increment by &#39; + convert(varchar(30), s.increment) end
   + case when (s.minimum_value is null) then &#39; no minvalue&#39; else &#39; minvalue &#39; + convert(varchar(30), s.minimum_value) end
   + case when (s.maximum_value is null) then &#39; no maxvalue&#39; else &#39; maxvalue &#39; + convert(varchar(30), s.maximum_value) end
   + case when (s.is_cycling = &#39;TRUE&#39;) then &#39; cycle&#39; else &#39; no cycle&#39; end
   + case when (s.is_cached = &#39;TRUE&#39;) then &#39; cache&#39; 
      + case when (s.cache_size is null) then &#39;&#39; else &#39; &#39; + convert(varchar(30), s.cache_size) end 
   else &#39; no cache&#39; end
from 
   sys.sequences s
[where 
   schema_name(s.schema_id) = %.q:SCHEMA%]
order by 1, 2
</a:TextTargetItem.Value>
<a:CreationDate>0</a:CreationDate>
