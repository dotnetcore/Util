<a:Name>Options</a:Name>
<a:TextTargetItem.Value>with : composite=yes, separator=yes, parenthesis=yes, chldmand=yes
{
pad_index=%s : list=on|off
fillfactor=%d : default=10
sort_in_tempdb=%s : list=on|off
ignore_dup_key=%s : list=on|off
statistics_norecompute=%s : list=on|off
drop_existing=%s : list=on|off
online=%s : list=on|off
allow_row_locks=%s : list=on|off
allow_page_locks=%s : list=on|off
maxdop=%s
&lt;compression&gt; : composite=yes, separator=yes
{
   data_compression = %s : list=none|row|page, composite=yes, multiple=yes
   {
      on partitions : composite=yes, parenthesis=yes, separator=yes
      {
         &lt;expression_or_range&gt; %s : multiple=yes
      }
   }
}
}
on  : composite=yes
{
   &lt;partition scheme&gt; %s : composite=yes, parenthesis=yes, chldmand=yes
   {
      &lt;column&gt; %s
   }
   &lt;filegroup&gt; %s : category=storage, default=&quot;DEFAULT&quot;
}
filestream_on : composite=yes, chldmand=yes
{
   &lt;partition scheme&gt; %s : composite=yes, parenthesis=yes, chldmand=yes
   {
      &lt;column&gt; %s
   }
  &lt;filegroup&gt; %s : category=storage, default=&quot;DEFAULT&quot;
}</a:TextTargetItem.Value>
