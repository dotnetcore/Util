<a:GetMethodScript>Function %Get%(obj)

   dim sget
   sget = obj.EvaluateTextFor(&quot;[%XMLSchemaCollection.Owner.GeneratedName%.]%XMLSchemaCollection.GeneratedName%&quot;)
   if(sget &lt;&gt; &quot;&quot;) then
      sget = obj.GetExtendedAttribute(&quot;ContentType&quot;) &amp; &quot; &quot; &amp; sget
   end if
   
   %Get% = sget

End Function</a:GetMethodScript>
<a:SetMethodScript>Sub %Set%(obj, value)
   
   dim rxp
   set rxp = new regexp
   rxp.IgnoreCase = true
output value
   rxp.Pattern = &quot;(content\s+|document\s+)?([^\.]+\.)?([^\.]+)&quot;
   if(rxp.Test(value)) then
output &quot;Match !&quot;
      dim mch, arr, xsc
      mch = rxp.Execute(value)(0)
      arr = Split(rxp.Replace(mch, &quot;$1;$2;$3&quot;), &quot;;&quot;)
      
      if (arr(1) &lt;&gt; &quot;&quot;) then arr(1) = Left(arr(1), Len(arr(1)) - 1)
      rxp.Pattern = &quot;\[([^\]]+)\]&quot;
      if(rxp.Test(arr(2))) then arr(2) = rxp.Replace(rxp.Execute(arr(2))(0), &quot;$1&quot;)
      if(rxp.Test(arr(1))) then arr(1) = rxp.Replace(rxp.Execute(arr(1))(0), &quot;$1&quot;)
       
      rxp.Pattern = &quot;\&quot;&quot;([^\&quot;&quot;]+)\&quot;&quot;&quot;
      if(rxp.Test(arr(2))) then arr(2) = rxp.Replace(rxp.Execute(arr(2))(0), &quot;$1&quot;)
      if(rxp.Test(arr(1))) then arr(1) = rxp.Replace(rxp.Execute(arr(1))(0), &quot;$1&quot;)
      
      &#39;xsc = Trim(arr(1)) &amp; &quot;.&quot; &amp; Trim(arr(2))
      xsc = Trim(arr(2))
      if (xsc &lt;&gt; &quot;&quot;) then
         obj.SetExtendedAttributeText &quot;XMLSchemaCollection&quot;, xsc
         output &quot;Set XMLSchemaCollection to &quot; &amp; xsc
         if(arr(0) &lt;&gt; &quot;&quot;) then
            obj.SetExtendedAttributeText &quot;ContentType&quot;, Trim(arr(0))
         end if
      end if
   end if
output &quot;exit&quot;
End Sub</a:SetMethodScript>
<a:Label>Hidden</a:Label>
