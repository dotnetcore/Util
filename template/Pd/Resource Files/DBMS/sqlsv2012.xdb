create fulltext index on [%TABLQUALIFIER%]%TABLE% (%CIDXLIST%)
   key index %INDEX%[%FullTextCatalog%?
      on %FullTextCatalog.GeneratedName%][
      with (%WithOptions%)]
:
[%Type%=XML?
create [%XMLPrimary%?primary ]xml index %INDEX% on [%TABLQUALIFIER%]%TABLE% (%CIDXLIST%)[%XMLPrimary%?:[%PrimaryXMLIndex%?
   using xml index %PrimaryXMLIndex.GeneratedName%[
      for %SecondaryXMLIndexType%]]]
:
[%Type%=SPATIAL?
create spatial index %INDEX% on [%TABLQUALIFIER%]%TABLE% %.A:SpatialIndexDefn%
:
create [%UNIQUE% ][%CLUSTER%?clustered :[nonclustered ]]index %INDEX% on [%TABLQUALIFIER%]%TABLE%[ (%CIDXLIST%)][
   include (%IncludedColumns%)][%FilterPredicate?%?[
   where %FilterPredicate%]][
   with (%WithOptions%)][
   on %OnOptions%]
]
]
]
]

</a:TextTargetItem.Value>
