<a:GetMethodScript>Function %Get%(obj)

   dim stpl
   stpl = &quot;[%Authorization%?authorization %Authorization.GeneratedName% ]&quot;
   stpl = stpl &amp; vbCrLf &amp; &quot;[%.l:Source%=provider?from %.l:Source% %Provider% &quot;
   stpl = stpl &amp; vbCrLf &amp; &quot;   with [creation_disposition = %CreateDisposition%, ]provider_key_name = %.q:ProviderKeyName%:[%Source%=Assembly?from assembly %Assembly.GeneratedName%:from [%.Q:Source%=&quot;&quot;Executable file&quot;&quot;?executable ]file = %.q:AssemblyFile%][%Algorithm%?&quot;
   stpl = stpl &amp; vbCrLf &amp; &quot;   with algorithm = %Algorithm%]][%EncryptionPassword%?&quot;
   stpl = stpl &amp; vbCrLf &amp; &quot;encryption by password = %.q:EncryptionPassword%] &quot;

   %Get% = obj.EvaluateTextFor(stpl)
End Function</a:GetMethodScript>
<a:SetMethodScript>Sub %Set%(obj, value)

   dim rxp, mch, str, arr
   set rxp = new regexp
   rxp.global = true
   rxp.IgnoreCase = true
   str = replace(replace(replace(value, vbTab, &quot; &quot;), vbLf, &quot; &quot;), vbCr, &quot; &quot;)
   rxp.Pattern = &quot;&quot;
   rxp.Pattern = rxp.Pattern &amp; &quot;(?:authorization\s+(\[[^\]]+\]|\w+))?&quot;
   rxp.Pattern = rxp.Pattern &amp; &quot;(?:from\s+file\s*=\s*&#39;([^&#39;]+)&#39;)?&quot;
   rxp.Pattern = rxp.Pattern &amp; &quot;(?:from\s+executable\s+file\s*=\s*&#39;([^&#39;]+)&#39;)?&quot;
   rxp.Pattern = rxp.Pattern &amp; &quot;(?:from\s+assembly\s+(\[[^\]]+\]|\w+))?&quot;
   rxp.Pattern = rxp.Pattern &amp; &quot;(?:from\s+provider\s+(\[[^\]]+\]|\w+))?&quot;
   rxp.Pattern = rxp.Pattern &amp; &quot;(?:algorithm\s*=\s*(rsa_512|rsa_1024|rsa_2048))?&quot;
   rxp.Pattern = rxp.Pattern &amp; &quot;(?:provider_key_name\s*=\s*&#39;([^&#39;]+)&#39;)?&quot;
   rxp.Pattern = rxp.Pattern &amp; &quot;(?:creation_disposition\s*=\s*(create_new|open_existing))?&quot;
   rxp.Pattern = rxp.Pattern &amp; &quot;(?:encryption\s+by\s+password\s*=\s*&#39;([^&#39;]+)&#39;)?&quot;

   for each mch in rxp.Execute(str)
      arr = split(rxp.Replace(mch, &quot;$1~$2~$3~$4~$5~$6~$7~$8~$9&quot;), &quot;~&quot;)
      if(arr(0) &lt;&gt; &quot;&quot;) then
         dim usr
         Set usr = obj.Model.FindChildByCode(arr(0), PDPDM.cls_User, &quot;User&quot;, Nothing, False, True)
         if(usr is Nothing) then
            Set usr = obj.Model.GetCollectionByStereotype(&quot;User&quot;).CreateNew()
            usr.SetNameAndCode arr(0), arr(0), True
         end if
         obj.SetExtendedAttribute &quot;Authorization&quot;, usr
      end if
      if(arr(1) &lt;&gt; &quot;&quot;) then
         obj.SetExtendedAttribute &quot;Source&quot;, &quot;File&quot;
         obj.SetExtendedAttributeText &quot;AssemblyFile&quot;, arr(1)
      end if
      if(arr(2) &lt;&gt; &quot;&quot;) then
         obj.SetExtendedAttribute &quot;Source&quot;, &quot;Executable file&quot;
         obj.SetExtendedAttributeText &quot;AssemblyFile&quot;, arr(2)
      end if
      if(arr(3) &lt;&gt; &quot;&quot;) then
         obj.SetExtendedAttribute &quot;Source&quot;, &quot;Assembly&quot;
         obj.SetExtendedAttributeText &quot;Assembly&quot;, arr(3)
      end if
      if(arr(4) &lt;&gt; &quot;&quot;) then
         obj.SetExtendedAttribute &quot;Source&quot;, &quot;Provider&quot;
         obj.SetExtendedAttribute &quot;KeyOption&quot;, &quot;Provider&quot;
         obj.SetExtendedAttribute &quot;Provider&quot;, arr(4)
      end if
      if(arr(5) &lt;&gt; &quot;&quot;) then
         obj.SetExtendedAttribute &quot;KeyOption&quot;, &quot;Algorithm&quot;
         obj.SetExtendedAttribute &quot;Algorithm&quot;, arr(5)
      end if
      if(arr(6) &lt;&gt; &quot;&quot;) then
         obj.SetExtendedAttribute &quot;KeyOption&quot;, &quot;Provider&quot;
         obj.SetExtendedAttribute &quot;ProviderKeyName&quot;, arr(6)
      end if
      if(arr(7) &lt;&gt; &quot;&quot;) then
         obj.SetExtendedAttribute &quot;KeyOption&quot;, &quot;Provider&quot;
         obj.SetExtendedAttribute &quot;CreateDisposition&quot;, arr(7)
      end if
      if(arr(8) &lt;&gt; &quot;&quot;) then
         obj.SetExtendedAttribute &quot;EncryptionPassword&quot;, arr(8)
      end if
      
   next


End Sub</a:SetMethodScript>
